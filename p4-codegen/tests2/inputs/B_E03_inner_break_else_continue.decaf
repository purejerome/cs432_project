def int main() {
  int i; int j; int s;
  i = 0; s = 0;
  while (i < 3) {
    j = 0;
    while (j < 4) {
      if (j == 1) { break; }     // leaves inner; outer continues body
      else         { j = j + 1; continue; }
      s = s + 100;               // unreachable
    }
    s = s + 10;                  // runs once per i
    i = i + 1;
  }
  return s; // expected: inner always breaks at j==1 on first iter => s += 10 per i => 30
}
