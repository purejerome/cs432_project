int x;
bool y[10];
int d[10];

def int doubleIt(int a) {
    return a * 2;
}

def int compute(int y) {
    int z;
    z = y + 5;       // (1) z is now live
    x = doubleIt(z); // (2) z is used as argument for a call
                     // (3) z is still needed after the call
    return z + x;    // (4) z must survive the call
}

def int main() {
    bool z;
    int a;
    y[compute(3)] = false;
    z = y[compute(3)];
    if(z){
        d[compute(3)] = 2;
        a = d[compute(3)];
    } else {
        d[compute(3)] = 5;
        a = d[compute(3)];
    }
    return a;
}
