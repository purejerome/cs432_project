def int foo2(int x) {
    return x + 1;
}

def int nameShadowEdge() {
    int foo;
    int result;
    foo = 10;
    // use function name even though variable exists
    result = foo2(foo); // should call the *function* foo(10), not load variable foo
    return result;
}

def int main() {
    bool love;
    love = !true;
    return nameShadowEdge();
}
