// C03_nested_while_loops.decaf
// Nested while loops to exercise live ranges across loop nests.

def int main()
{
    int i;
    int j;
    int sum;
    int temp;

    i = 1;
    sum = 0;

    while (i <= 3)
    {
        j = 1;
        while (j <= 2)
        {
            temp = i * j;
            sum = sum + temp;
            j = j + 1;
        }
        i = i + 1;
    }

    // sum = 1*1 + 1*2 + 2*1 + 2*2 + 3*1 + 3*2 = 1+2+2+4+3+6 = 18
    print_int(sum);
    print_str("\n");

    return sum;
}
