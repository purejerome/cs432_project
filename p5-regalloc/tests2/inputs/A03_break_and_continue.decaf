// A03_break_and_continue.decaf
// Complex loops with break and continue in nested structure.

def int main()
{
    int i;
    int j;
    int sum;
    int count;

    i = 0;
    sum = 0;
    count = 0;

    while (i < 5)
    {
        j = 0;
        while (j < 5)
        {
            if (i + j == 3)
            {
                j = j + 1;
                continue;       // skip this pair
            }

            if (i == 4 && j == 4)
            {
                break;          // break inner loop final corner
            }

            sum = sum + (i * 10 + j);
            count = count + 1;

            j = j + 1;
        }

        i = i + 1;
    }

    // Just return a deterministic combination of sum and count
    print_int(sum);
    print_str(" ");
    print_int(count);
    print_str("\n");

    return sum + count;
}
