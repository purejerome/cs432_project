// C05_array_index_arith.decaf
// Array with computed indices and multiple live values.

int arr[5];

def int main()
{
    int i;
    int sum;
    int idx1;
    int idx2;

    arr[0] = 1;
    arr[1] = 2;
    arr[2] = 3;
    arr[3] = 4;
    arr[4] = 5;

    idx1 = 1 + 1;        // 2
    idx2 = 4 - 1;        // 3

    sum = 0;
    sum = sum + arr[idx1];  // arr[2] = 3
    sum = sum + arr[idx2];  // arr[3] = 4
    sum = sum + arr[0];     // 1

    // sum = 3 + 4 + 1 = 8
    print_int(sum);
    print_str("\n");

    return sum;
}
