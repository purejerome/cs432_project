// A12: nested loops with valid break/continue usage -> accept
def int main() {
    int i;
    i = 3;
    while (i > 0) {
        int j;
        j = i;
        while (j > 0) {
            if (j == 2) { j = j - 1; continue; }
            if (j == 1) { break; }
            j = j - 1;
        }
        i = i - 1;
    }
    return 0;
}
