int[5] B;
def void v() { return; }
def int main() {
  int x;
  B[0] = 2;
  x = B[B[0] < 3];        // bool index
  x = B[v() + 1];         // void in arithmetic
  x = B[B] + B[true];     // array name as index, bool index
